{
  "name": "clowncryption",
  "packageManager": "yarn@3.2.0",
  "version": "2.0.1",
  "main": "./dist/index.js",
  "scripts": {
    "build": "yarn prettier --write ./src && yarn tsup --dts && npx typedoc src/index.ts --options typedoc.json",
    "BND": "yarn tsup --dts"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "prettier": "^2.7.1",
    "tsup": "^6.4.0",
    "typedoc": "^0.23.20",
    "typescript": "^4.8.3"
  },
  "directories": {
    "doc": "docs",
    "lib": "dist"
  },
  "files": ["dist"],
  "author": "Brady Bangasser",
  "license": "MIT",
  "typings": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/BradyBangasser/ClownCryption"
  },
  "readme": "./README.md",
  "homepage": "https://clowncryption.bangasser.dev",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  }
